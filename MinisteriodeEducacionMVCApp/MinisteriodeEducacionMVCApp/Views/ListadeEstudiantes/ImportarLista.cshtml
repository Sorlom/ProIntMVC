@model MinisteriodeEducacionMVCApp.Models.ListadeEstudiantes
@{
    ViewBag.Title = "ImportarLista";
}

<h2>ImportarLista</h2>


@using (Html.BeginForm("ImportarLista", "ListadeEstudiantes", FormMethod.Post))
{ 
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Lista de Estudiantes</h4>
        <hr />


        @Html.Editor("Pato", new { htmlAttributes = new { @class = "form-control" } })  
         
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Importar" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Volver a la Lista", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
